%p#notice= notice
%h1 Artworks
%table
  %thead
    %tr
      %th Style
      %th Image
      %th Model
      %th Size
      %th Smoothness
      %th Num iters
      %th{colspan: "3"}
  %tbody
    - @artworks.each do |artwork|
      %tr
        %td= artwork.style_id
        %td= artwork.image_id
        %td= artwork.model
        %td= artwork.size
        %td= artwork.smoothness
        %td= artwork.num_iters
        %td= link_to 'Show', artwork
        %td= link_to 'Edit', edit_artwork_path(artwork)
        %td= link_to 'Destroy', artwork, method: :delete, data: { confirm: 'Are you sure?' }
%br/
= link_to 'New Artwork', new_artwork_path
