%p#notice= notice
%h1 Api Keys
%table
  %thead
    %tr
      %th Token
      %th{colspan: "3"}
  %tbody
    - @api_keys.each do |api_key|
      %tr
        %td= api_key.token
        %td= link_to 'Destroy', api_key, method: :delete, data: { confirm: 'Are you sure?' }
%br/
= link_to 'New Api Key', api_keys_path, method: :post
